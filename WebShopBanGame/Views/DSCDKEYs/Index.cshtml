@model IEnumerable<WebShopBanGame.Models.DSCDKEY>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <div class="table-responsive">
            <table class="table table-bordered">
                <tr>
                    <td>Mã Hóa đơn</td>
                    <th>Hình</th>
                    <th>Tên sản phẩm</th>
                    <th>CD-Key</th>                 
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.MaHD</td>
                        <td><img src="@item.SANPHAM.Anh1" width="100" height="60" /> </td>
                        <td><a target="_blank" href="@Url.Action("Details", "SANPHAMs", new { id = @item.MaSP })"> @item.SANPHAM.TenSP </a> </td>
                        <td>
                            @item.CDKey
                        </td>                        
                    </tr>
                }               
            </table>          
        </div>
    </div>
</div>

